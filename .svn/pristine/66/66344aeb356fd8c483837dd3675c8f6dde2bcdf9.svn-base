*{padding:0;margin:0}
body{font:14px/1.5 "Microsoft YaHei",NSimSun,SimHei,Verdana,Arial,sans-serif;color:#333;background-color:#f2f2f2}
b,h1,h2,h3,h4,h5,h6,strong,th{font-weight:400}
em,i{font-style:normal}
ul,ol{list-style:none}
button,input[type=button]{border:none;outline:0;cursor:pointer}
/*A*/
a{text-decoration:none}
.absolute{position:absolute}

/*B*/
.block-center{margin:0 auto}
.border-bottom-dashed{border-bottom:1px dashed #ccc}
.border-bottom-solid{border-bottom:1px solid #ccc}
.botton-orange{background-color:#ff6d22;border-color:#ff6d22;color:#fff}
.botton-orange:focus,.botton-orange:hover{background-color:rgba(255,108,34,.8);border-color:#ff6d22;color:#fff}
.bold{font-weight: bold}

/*C*/
.color-red{color:#ff313b}
.color-orange{color:#FF6D22}
.color-green{color:#3DC68A}
.color-gray{color:#6A6A6A}
.color-grey{color:#999}
.color-black{color: #333}
.color-blue{color:#0086e4}
.color-white{color:#fff}
.col{font-size:0}
.col-5{width:50%;display:inline-block}

/*D*/
.delete-line{text-decoration: line-through}

/*E*/
.el-button--orange{color:#fff;background-color:#ff6d22;border-color:#ff6d22}
.el-button--orange:focus,.el-button--orange:hover{background:#ff904f;border-color:#ff904f;color:#fff}
.el-button--orange.is-active,.el-button--orange:active{background:#ff6d22;border-color:#ff6d22;color:#fff}
.el-button--orange:active{outline:0}
.el-button--orange.is-plain{color:#ff6d22;background:#fff;border-color:#ff6d22}
.el-button--orange.is-plain:focus,.el-button--orange.is-plain:hover{background:#ff6d22;border-color:#ff6d22;color:#fff}
.el-button--orange.is-plain:active{background:#ff6d22;border-color:#ff6d22;color:#fff;outline:0}
.el-dialog{width: 600px;margin-top:30vh}
.el-dialog__header{padding: 10px 20px;background-color: #409eff}
.el-dialog__body{	padding:20px;}
.el-dialog__title{font-size: 16px;color: #fff}
.el-dialog__headerbtn{top: 15px;}
.el-dialog__headerbtn .el-dialog__close{color: #fff}
.el-dialog__headerbtn:hover .el-dialog__close,.el-dialog__headerbtn:focus .el-dialog__close{color: #fff}
.el-dialog__footer{padding: 0 20px 20px 20px}
.el-form-item[is-required] > label::before,label[required]::before{content:'*';color:#f56c6c;margin-right:4px}
.el-message-box__content{padding:0 20px;}
.el-pagination{margin:20px auto}
.el-rate{height: 26px;display: inline-block;margin-left: 8px;vertical-align: middle;}
.el-rate__icon{font-size: 24px;}
.el-steps .el-step__icon{height:32px;width:32px}
.el-steps>div:first-child>div:first-child{position:relative}
.el-steps>div:first-child>div:first-child::before{content:"";height:2px;width:50%;position:absolute;top:15px;left:0;background-color:#409eff}
.el-steps>div:last-child>div:first-child{position:relative}
.el-steps>div:last-child>div:first-child::after{content:"";height:2px;width:50%;position:absolute;top:15px;right:0;background-color:#c0c4cc}
.el-steps>div:last-child>div.is-success:first-child::after{background-color:#409eff}
.el-step__head.is-success{color:#409eff;border-color:#409eff}
.el-step__title.is-process,.el-step__title.is-success{color:#409eff}
.el-step__title.is-process{font-size:16px}
.el-step.is-horizontal .el-step__line{top:15px}
.el-step__description.is-success{color:#999}
.el-step__description{width:80px;margin:0 auto}
.el-step__head.is-process{color:#fff;border-color:#409eff;position:relative}
.el-step__head.is-process .el-step__icon{background-color:#409eff}
.el-step__head.is-process::before{content:"";position:absolute;height:2px;top:15px;left:-50%;width:100%;background-color:#409eff}
.el-step__title.is-process,.el-tabs__item{font-weight:400}
.el-tabs__item.is-active{font-size:16px}
.el-tabs__nav-next, .el-tabs__nav-prev{font-size: 22px; line-height: 50px;}


.evaluate-item >div{display:inline-block;width:230px;text-align:center;box-sizing:border-box;border:1px solid #dcdfe6;border-radius:4px;font-size:12px;height:40px;line-height:40px;margin:0 10px 10px 0;cursor:pointer;position:relative}
.evaluate-item >div:nth-child(2n){margin-right:0}
.check::before{content:'';position:absolute;bottom:0;right:0;z-index:1;width:0;height:0;border-bottom:18px solid #409eff;border-left:18px solid transparent}
.check::after{content:'\E611';font-family:element-icons!important;position:absolute;bottom:0;right:0;z-index:2;color:#fff;font-size:8px;line-height:10px}

/*F*/
.font14{font-size: 14px}
.font15{font-size: 15px}
.font16{font-size: 16px}
.font20{font-size: 20px !important}

/*I*/
.icon{background-image:url(../../assets/images/icon.png)}
.icon-order{background-position: 0 0;width:22px;height:22px;display:inline-block}
.icon-droit{background-position: 0 -32px;width:22px;height:22px;display:inline-block}
.icon-settlement{background-position: 0 -64px;width:22px;height:22px;display:inline-block}
.icon-center{background-position: 0 -96px;width:22px;height:22px;display:inline-block}
.icon-bond{background-position: 0 -184px;width:136px;height:25px;display:block;color:#C3800F;text-indent:28px;font-size: 12px;line-height: 25px}
.icon-mark{background-position: -341px 0;width:14px;height:14px;display:inline-block;}
.icon-int{background-position: -330px -83px;width:15px;height:22px;display:inline-block;}

/*L*/
.list{color: #666;}
.list li{font-size:14px;line-height: 21px;padding:5px 0;position: relative;}
.list li[label]{padding-left: 80px;}
.list li[label]::before{position: absolute;left:0;content:attr(label)}
.lineheight-three{line-height: 3em}
/*M*/
.margin-bottom{margin-bottom: 1em}
.margin-level{margin-left:1em;margin-right: 1em}
.margin-right{margin-right: 1em}
.margin-top{margin-top: 1em}
.margin-vertical{margin-bottom: 1em;margin-top: 1em}
.margin-vertical-half{margin-bottom: .5em;margin-top: .5em}
.main{font-size:0}
.main-content{background-color: #fff;	padding: 30px;}
.main-content-right{display: inline-block;vertical-align: top;margin-left: 10px;width: 1038px;box-sizing:border-box;background-color: #fff;	padding: 20px;box-sizing: border-box;}

/*O*/
.order-list{padding:20px;background-color:#fff;font-size:14px}
.order-list .order-title{background-color:#f5f5f5;border:1px solid #e8e8e8;font-size:0}
.order-list .order-title li{display:inline-block;font-size:14px;text-align:center;height:50px;line-height:50px}
.order-list .order-title li .el-date-editor{margin-left:10px;width:245px}
.order-list .order-title li .el-date-editor input{width:80px}
.order-list .order-title li:nth-child(1){width:155px}
.order-list .order-title li:nth-child(2){width:325px}
.order-list .order-title li:nth-child(3){width:145px}
.order-list .order-title li:nth-child(4){width:145px}
.order-list .order-title li:nth-child(5){width:225px}
.order-list .el-message-box__content{padding:0 15px}
.order-list .el-tabs__item{font-size:15px;font-weight:700;line-height:50px;height:50px}
.order-list .el-tabs__item.is-active{font-size:15px}
.order-item{margin-top:10px;background-color:#daf3ff;border-collapse:separate;border-spacing:1px;border:1px solid #fff}
.order-item:hover{border-color:#0086e4}
.order-item th{background-color:#eaf8ff;height:43px;line-height:43px;background-color:#eaf8ff;font-family:SimSun;text-align:left;text-indent:14px;font-weight:400;position:relative}
.order-item th>*{display:inline-block}
.order-item th strong{font-weight:700;margin-right:3em}
.order-item th span{width:200px}
.order-item td{height:145px;background-color:#fff;vertical-align:middle;text-align:center}
.order-item td>*{margin:8px}
.order-item td>a:nth-child(1){width:68px;height:34px;background-color:#ff6d22;margin:0 auto;border-radius:4px;color:#fff;line-height:34px}
.order-item td p{text-align:left;padding-left:10px}
.order-item a{text-decoration:none;color:#ff6d22;display:block}


/*P*/
.padding-bottom{padding-bottom: 1em}
.padding-left{padding-left: 1em}
.progress{padding:30px 0}
.personal-content {width:600px;margin-top: 30px;}
/*R*/
.relative{position:relative}
.right{right:0}

/*S*/
.search{padding-bottom:15px;font-size:14px}
.search>label{display:inline-block;margin-right:10px;text-align:right}
.search .el-input{width:228px;margin-right:35px}
.search .el-input>input{height:34px}
.search .el-button{height:34px;width:64px;background-color:#0086e4;color:#fff;z-index:0;padding:0;border-color:#0086e4}
.summary{box-sizing:border-box;background-color:#e2f3ff;border:solid 1px #64bbf8;padding:20px;font-size:14px}
.summary h4{font-size:20px;color:#333;font-weight:700;margin-bottom:15px}
.summary p{margin-bottom:15px}
.summary button{width:114px;height:34px;background-color:#ff6d22;color:#fff;border-radius:4px;border-color:#ff6d22;margin-right:15px}
.summary button:focus,.summary button:hover{color:#fff;background-color:rgba(255,108,34,.8);border-color:#ff6d22}
.summary a{color:#FF6D22;margin-left: 16px;}
.summary a:hover{text-decoration:underline}
.summary a::after{content:'|';padding-left:16px;color:#ccc}
.summary a:last-child::after{content:''}

/*T*/
.table tr[align="left"] th,.table tr[align="left"] td{text-align: left;padding-left: 20px;}
.table th{line-height:42px;text-align:center;color:#000;border-width: 0;border-style:solid;border-color: transparent; border-top-width: 1px; border-bottom-width: 1px;}
.table th:first-child{border-left-width:1px}
.table th:last-child{border-right-width:1px}
.table td{border-width: 0;border-style:solid;border-color: transparent;border-bottom-width:1px;border-left-width:1px;vertical-align:middle;text-align:center}
.table td:last-child{border-right-width:1px}
.table td a,.title a,.list a{color: #ff6d22;cursor: pointer}
.table td a:hover,.title a:hover,.list a:hover{color: #ff6d22;text-decoration: underline;}
.table-border{background-color: #e8e8e8;border-spacing: 1px 1px;}
.table-border td{background-color: #fff; vertical-align: middle;padding: 8px 0;}
.theme-bluegray td,.theme-bluegray th{border-color:#e0eaf6}
.theme-bluegray th{background-color:#f2f7ff}
.theme-gray th{background-color: #f5f5f5}
.theme-gray td,.theme-gray th{border-color: #e8e8e8}
.text-center{text-align:center}
.text-indent{text-indent:1em}
.text-two-indent{text-indent:2em}
.text-left{text-align:left !important}
.text-right{text-align:right}

.title{font-size:16px;line-height:30px;box-sizing:border-box;position:relative}
.title a,.title label{position:absolute;right: 10px;font-size: 14px;}
.top{top:0}

/*W*/
.w{width:1200px;box-sizing:border-box}